{
  "name": "react-web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "set PORT=5000 && craco start",
    "craco-start": "set PORT=5000 && craco start",
    "craco-build": "set GENERATE_SOURCEMAP=false && craco build",
    "craco-test": "craco test",

    "script-start": "react-scripts start",
    "script-build": "react-scripts build",
    "script-test": "react-scripts test",
    "script-eject": "react-scripts eject",

    "rewired-start": "react-app-rewired start",
    "rewired-build": "react-app-rewired build",
    "rewired-test": "react-app-rewired test",
    "rewired-eject": "react-app-rewired eject",

    "lint": "vue-cli-service lint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{js,vue,ts,jsx,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{ts,jsx,js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:style:lint": "stylelint --fix \"src/**/*.{ts,js,tsx,jsx,vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",

    "reset:install": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && yarn install",
    "jenkins-serve": "node ./z-generate/z-node-serve-build.js",
    "generate-serve": "node ./z-generate/serve/node-api-serve.js",
    "g-serve": "concurrently \"npm run vue-serve\" \"npm run generate-serve\" ",
    "node-serve": "node ./z-node-local-server.js -- product_name=MG-TS-VUE-BACK-CLI/dist product_host=8100"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "redux-alita": "^2.3.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "react-jsx": "1.0.0",
    "react-app-polyfill": "^1.0.4",
    "react-dev-utils": "^9.1.0",
    "lodash-es": "^4.17.20",
    "js-cookie": "2.2.1",
    "web-vitals": "^1.0.1",
    "axios": "^0.19.2",
    "xlsx": "^0.16.9",
    "mockjs2": "^1.0.8",
    "qs": "6.9.6"
  },
  "devDependencies": {
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/react-beautiful-dnd": "^12.1.2",
    "@types/react-color": "^3.0.1",
    "@types/react-document-title": "^2.0.3",
    "@types/react-draft-wysiwyg": "^1.12.3",
    "@types/react-loadable": "^5.5.1",
    "@types/react-router-dom": "^5.1.2",
    "@types/node": "^12.12.6",
    "@types/lodash-es": "^4.17.4",
    "@types/mockjs": "^1.0.3",
    "@types/webpack-env": "1.16.0",

    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-react-app": "^9.0.2",

    "@craco/craco": "^5.5.0",
    "craco-less": "^1.17.1",
    "craco-alias": "^2.2.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "react-app-rewired": "^2.1.5",
    "customize-cra": "^0.9.1",

    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.2.1",

    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",

    "postcss-import": "12.0.1",
    "postcss-loader": "^2.0.8",
    "postcss-pxtorem": "^4.0.1",

    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",

    "typescript": "^4.2.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2"
  }
}
